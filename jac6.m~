function jac = jac6(model,types)
z(:,:,1) = [0;0;0];
o(:,:,1) = [0;0;0];
%def relevent vectors
dof = length(model(1,1,:))
z(:,:,2:8) = model(1:3,3,:);
o(:,:,2:8) = model(1:3,4,:);
jac
%check for revolute or prismatic
for i = 2:dof+1
    if types(i) == 0
        jac(1:,i) = cross(z(:,:,i-1),model(1:3,4,dof));
        jac(2,i) = z(:,:,i-1);
    else
        jac(1,i) = z(:,:,i-1); 
        jac(2,i) = [0;0;0];
    end
end

end
